{
  "printWidth": 120,
  "semi": false,
  "space-before-function-paren": 0,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "insertPragma": false,
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "lf",
  "stylelintIntegration": true,
  "eslintIntegration": true,
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "ignore",
  "quoteProps": "as-needed"
}
